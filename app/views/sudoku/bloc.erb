<td data-bloc="<%= blocId %>" data-x="<%= xb=blocId/3 %>" data-y="<%= yb=blocId%3 %>">
    <table class="inner" border="1">
        <% 3.times do |i| %>
            <tr>
                <% bloc.select{|c| c.line==(3*xb+i) }.each do |c| %>
                    <%= erb :'sudoku/box', :locals => {:box => c} %>
                <% end %>
            </tr>
        <% end %>
    </table>
</td>